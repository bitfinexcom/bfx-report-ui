{
  "name": "bfx-report-ui",
  "version": "0.1.0",
  "description": "Report page to overview the user actions in Bitfinex and download related csv files",
  "repository": {
    "type": "git",
    "url": "https://github.com/bitfinexcom/bfx-report-ui.git"
  },
  "keywords": [
    "bitfinex"
  ],
  "author": "Bitfinex",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/bitfinexcom/bfx-report-ui/issues"
  },
  "homepage": "https://github.com/bitfinexcom/bfx-report-ui#readme",
  "dependencies": {
    "@blueprintjs/core": "^3.6.1",
    "@blueprintjs/datetime": "^3.2.0",
    "@blueprintjs/icons": "^3.1.0",
    "@blueprintjs/select": "^3.2.0",
    "@blueprintjs/table": "^3.1.1",
    "classnames": "^2.2.6",
    "flexboxgrid2": "^7.2.1",
    "history": "^4.7.2",
    "lodash": "^4.17.10",
    "moment": "^2.22.2",
    "node-sass-chokidar": "^1.3.3",
    "normalize.css": "^8.0.0",
    "prop-types": "^15.6.1",
    "query-string": "^5.1.1",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-intl": "^2.4.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-scripts": "1.1.4",
    "react-transition-group": "^2.3.1",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-persist": "^5.10.0",
    "redux-saga": "^0.16.0"
  },
  "devDependencies": {
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^5.0.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.9.1",
    "husky": "^0.14.3",
    "react-test-renderer": "^16.4.1"
  },
  "scripts": {
    "build-sass": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-sass": "npm run build-sass && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "start": "npm run build-sass && react-scripts start",
    "build": "npm run build-sass && react-scripts build",
    "test": "react-scripts test --env=jsdom --modulePaths=src",
    "lint": "eslint src --ignore-path .gitignore",
    "lintFix": "eslint src --fix --ignore-path .gitignore",
    "prepush": "npm run lint"
  }
}
